<h1>登録情報編集</h1>

<%= form_for(@member, url: members_members_path) do |f| %>
  <h4>名前</h4>
  <p>(性)</p>
  <%= f.text_field :family_name %>
  <p>(名)</p>
  <%= f.text_field :first_name %>
  <h4>フリガナ</h4>
  <p>(セイ)</p>
  <%= f.text_field :kana_family_name %>
  <p>(メイ)</p>
  <%= f.text_field :kana_first_name %>
  <h4>メールアドレス</h4>
  <%= f.text_field :email %>
  <h4>郵便番号（ハイフンなし）</h4>
  <%= f.text_field :postal_code %>
  <h4>住所</h4>
  <%= f.text_field :address %>
  <h4>電話番号（ハイフンなし）</h4>
  <%= f.text_field :telephone_number %>

    <%= f.submit '編集内容を保存する' class: "btn btn-primary"%>
<% end %>

  <%= link_to "退会する",'/members/withdraw',, class: "btn btn-danger" %>
