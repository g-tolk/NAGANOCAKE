<h3>ジャンル編集</h3>

<%= form_for(@genre, url: admin_genre_path, method: :patch) do |f| %>
  <%= f.text_field :name %>
  <label><%= f.radio_button :valid_status, "有効",{:checked => true} %>有効</label>
  <label><%= f.radio_button :valid_status, "無効" %>無効</label>
  <%= f.submit '変更を保存する', class:"btn btn-primary" %>
<% end %>

